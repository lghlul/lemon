<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.mapper.ClassInfoMapper">


    <insert id="save" parameterType="addClassDTO">
      insert into classInfo(classNumber , className  , createTime) values(#{classNumber} , #{className}  , #{createTime})
   </insert>

    <select id="get" parameterType="string" resultType="classInfo">
      select * from classInfo where classNumber = #{classNumber}
    </select>

    <update id="update" parameterType="updateClassDTO">
      update classInfo set className = #{className} where classNumber = #{classNumber}
    </update>

    <delete id="delete" parameterType="string">
      delete from classInfo where classNumber = #{classNumber}
    </delete>


</mapper>
