<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.mapper.ClassMapper">


    <insert id="saveClass" parameterType="addClassDTO">
      insert into class(classNumber , className  , createTime) values(#{classNumber} , #{className}  , #{createTime})
   </insert>

    <select id="getClass" parameterType="string" resultType="classDO">
      select * from class where classNumber = #{classNumber}
    </select>


</mapper>
